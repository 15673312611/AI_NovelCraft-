# AI服务配置
# 请复制此文件为 .env 并填入真实的配置信息

# AI API 基础地址
AI_BASE_URL=https://doradoapi.top

# AI API 密钥 (必须配置)
AI_API_KEY=your-api-key-here

# 默认AI模型
AI_DEFAULT_MODEL=gpt-4o-mini

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=ai_novel
DB_USERNAME=root
DB_PASSWORD=root

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DATABASE=0

# JWT配置
JWT_SECRET=novel-creation-system-jwt-secret-key-2024
JWT_EXPIRATION=86400000

# 服务器配置
SERVER_PORT=8080

# 日志级别
LOG_LEVEL=DEBUG
